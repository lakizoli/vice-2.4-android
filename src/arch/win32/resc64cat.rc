BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "04090000"
        BEGIN
            VALUE "CompanyName",     "Vice Team"
            VALUE "FileDescription", FILE_DESCRIPTION
            VALUE "FileVersion",     VERSION
            VALUE "ProductName",     FILE_DESCRIPTION
            VALUE "ProductVersion",  VERSION
        END
